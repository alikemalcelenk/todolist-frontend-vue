{
  "name": "todolist-frontend-vue",
  "version": "1.0.0",
  "description": "Todolist Web Client that is written in JavaScript using Vue.",
  "scripts": {
    "dev": "nuxt",
    "build": "yarn run build-storybook && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "format": "prettier --write '**/*.js{,x}' '**/*.vue'",
    "storybook": "nuxt storybook",
    "build-storybook": "nuxt storybook build -o ./static/guide",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.1.1",
    "@nuxtjs/style-resources": "^1.2.1",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/vercel-builder": "^0.22.1",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "jest": "^27.0.5",
    "postcss": "^8.3.8",
    "prettier": "^2.3.2",
    "sass": "^1.42.1",
    "sass-loader": "^10.1.1",
    "vue-jest": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alikemalcelenk/todolist-frontend-vue.git"
